    import Swiper from 'swiper';
    import 'swiper/css/swiper.min.css';
	export default {
		name: 'Home',
		components: {
			Footer,
			Top
		},
		data() {
			return {
				height: {
					height: window.innerHeight + 'px',
				},
				topClass: '',
				topClassOne: '',
				leftClass: '',
				leftClassOne: '',
				rightClass: '',
				rightClassOne: ''
			}
		},
		mounted() {
			var swiper = new Swiper('#case4', {
			    loop: true, //允许从第一张到最后一张，或者从最后一张到第一张  循环属性
			    slidesPerView: 4, // 设置显示三张
			    //centeredSlides : true,     //使当前图片居中显示
			    freeMode: true, // 使幻灯片滑动时不止滑动一格，且不会自动贴合 
			    slidesPerGroup: 1, //定义1张图片为一组
			    autoplay: false, //可选选项，自动滑动
			    speed: 6000, //设置过度时间
			    grabCursor: true, //鼠标样式根据浏览器不同而定 
			    autoplay: {
			        delay: 1,
			        disableOnInteraction: false,
			    },
			    scrollbar: {
			        el: '.swiper-scrollbar',
			        dragSize: 150
			    },
			});
			let w =$(window).width();
			shows();
			$(".home-banner-nav a:last,.home-nav-top>div a:last").addClass("nav_active");
			window.addEventListener('scroll', this.NavScrollFunction, true);
			$(".home-banner-btn").click(function(){
				$(".appointment").fadeIn();
			});
			$(".home-banner-btn2").click(function(){
				$(".appointment").fadeIn();
			});
			//儿童服务、教师培训、线上服务、科研教研
			$(".home-theme-up li").mouseenter(function(event){
				$(this).find("div:first img").stop().animate({
					"opacity":1,
					"right":"-6px"
				}).show();
				$(this).find("div:first p").stop().animate({"left":"0"});
				$(this).find("div:first span").stop().animate({"left":"0"});
				$(this).find("div:last span").stop().animate({
					"left":"0"
				}).show();
				event.stopPropagation();
			}).mouseleave(function(event){
				$(this).find("div:first img").stop().animate({
					"opacity":0,
					"right":"0"
				},function(){
					$(this).hide();
				});
				$(this).find("div:last span").stop().animate({
					"left":"-100%"
				}).show();
				$(this).find("div:first p").stop().animate({"left":"10px"});
				$(this).find("div:first span").stop().animate({"left":"10px"});
				event.stopPropagation();
			})
			$(".home-site ul li").mouseenter(function(event){
				$(this).stop().removeClass("stop_top").addClass("up_top");
				event.stopPropagation();
			}).mouseleave(function(event){
				$(this).stop().removeClass("up_top").addClass("stop_top");
				event.stopPropagation();
			})
		},
		methods: {
			NavScrollFunction() {
				if (document.body.scrollTop > 50 || document.documentElement.scrollTop > 50) {
					$(".home-banner-nav").hide();
					$(".home-nav-top").fadeIn(200);
				} else {
					$(".home-nav-top").hide();
					$(".home-banner-nav").fadeIn(200);
				}
				this.scrollnumber($('.home-site-title1'),'font_active_up',50,1);
				this.scrollnumber($('.home-site-title2'),'font_active_up',50,2);
			},
			scrollnumber(element, cssname, offset,type) {
			    let a, b, c, d;
			    d = $(element).offset().top;
			    a = eval(d + offset);
			    b = $(window).scrollTop();
			    c = $(window).height();
			    if (b + c > a) {
			        if(type == 1 && this.topClass == ""){
						this.topClass = 'font_active_up';
						this.leftClass = 'font_active_left';
						this.rightClass = 'font_active_right';
					}else if(type == 2 && this.topClassOne == ""){
						this.topClassOne = 'font_active_up';
						this.leftClassOne = 'font_active_left';
						this.rightClassOne = 'font_active_right';
					}
			    }
			},
			train(){
				let train=this.$router.resolve({path:'/train'});
				window.location.href=train.href;
			},
			product(){
				let product=this.$router.resolve({path:'/product'});
				window.location.href=product.href;
			},
			teacher(){
				let teacher=this.$router.resolve({path:'/teacher'});
				window.location.href=teacher.href;
			},
			sunhe(){
				this.$router.push({path:'/sunhe'});
				let train=this.$router.resolve({path:'/train'});
				window.location.href=train.href;
			},
			haidian(){
				let haidian=this.$router.resolve({path:'/haidian'});
				window.location.href=haidian.href;
			},
			recovery(){
				let childrenRecovery=this.$router.resolve({path:'/childrenrecovery'});
				window.location.href=childrenRecovery.href;
			},
			jingan(){
				let jingan=this.$router.resolve({path:'/jingan'});
				window.location.href=jingan.href;
			},
			huaqiaocheng(){
				let shenzhen=this.$router.resolve({path:'/shenzhen'});
				window.location.href=shenzhen.href;
			},
			goChangYang(){
				let chaoyang=this.$router.resolve({path:'/chaoyang'});
				window.location.href=chaoyang.href;
			}
		}
	}
</script>
