						},
						{
							min: 2,
							max: 10,
							message: '长度在 2 到 10 个字符',
							trigger: 'blur'
						}
					],
					value: [{
						required: true,
						message: '请选择性别',
						trigger: 'change'
					}],
					provinceAndCity: [{
						required: true,
						message: '请选择地址',
						trigger: 'change'
					}],
					value1: [{
						type: 'date',
						required: true,
						message: '请选择出生日期',
						trigger: 'blur'
					}],

					iphone: [{
							required: true,
							message: '请输入联系电话',
							trigger: 'blur'
						},
						{
							min: 11,
							max: 11,
							message: '请输入11位正确联系电话',
							trigger: 'blur'
						}
					],
					ideaValue: [{
						required: true,
						message: '请选择中心',
						trigger: 'change'
					}]
				},
				pickerOptions1: {
					disabledDate(time) {
						return time.getTime() > Date.now();
					}
				}
			}
		},
		mounted() {
			$(".close").click(function() {
				$(".appointment").fadeOut();
			})
			this.options = AddArr;
			$(".true-close img").click(function() {
				$(".appointment").fadeOut();
				$('.submit-true').hide();
				$('.appointment-view').show();
			})
		},
		methods: {
			submitForm(formName) {
				this.$refs[formName].validate((valid) => {
					if (valid) {
						$.ajax({
							type: "post",
							dataType: 'json',
							url: "http://app.ingcare.com/wap/common/research/children/submitFweb.do",
							data: $("#appointment").serialize(),
							success: function(data) {
								if (data.extension == 1) {
									$('.appointment-view').hide();
									$('.submit-true').show();
								} else if (data.extension == 2 || data.extension == '2') {
									$(".hint").html(data.message == '' ? '' : data.message);
								}
							},
							error: function(e) {
								console.log(e);
							}
						})
					} else {
						console.log('error submit!!');
						return false;
					}
				});
			},
			resetForm(formName) {
				this.$refs[formName].resetFields();
			}
		}
	}
</script>
