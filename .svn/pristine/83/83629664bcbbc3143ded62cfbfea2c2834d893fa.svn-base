				$(".site-shop-chaoyang-left-scroll-right-btn img:last").addClass('hidden');
				$(".site-shop-chaoyang-left-scroll-content ul").animate({marginLeft:"0px"},500);
			})
			$(".site-shop-chaoyang-left-scroll-content ul li").click(function(){
				let i = $(this).index();
				$(".site-shop-chaoyang-left-image img").eq(i).removeClass('hidden').siblings().addClass('hidden');
