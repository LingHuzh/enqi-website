							success: function(data) {
								if (data.extension == 1) {
									$('.appointment-view').hide();
									$('.submit-true').show();
								} else if (data.extension == 2 || data.extension == '2') {
									$(".hint").html(data.message == '' ? '' : data.message);
								}
							},
							error: function(e) {
