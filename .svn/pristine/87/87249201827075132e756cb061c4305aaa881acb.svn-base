			scrollnumber(element, cssname, offset, type) {
				let a, b, c, d;
				d = $(element).offset().top;
				a = eval(d + offset);
				b = $(window).scrollTop();
				c = $(window).height();
				if (b + c > a) {
					if (type == 1 && this.topClass == "") {
						this.topClass = 'font_active_up';
