			},
			aboutusScroll(){
				let url = location.href;
				let index = url.lastIndexOf("=");
				let subBack = url.substring(index + 1, url.length);
				console.log(subBack);
				if (subBack == 1) {
					setTimeout(()=>{
						document.documentElement.scrollTop = 500
					},500)
				}else if(subBack == 2){
					setTimeout(()=>{
						document.documentElement.scrollTop = 1200
					},500)
				}else if(subBack == 3){
