			autoscroll();
			var v = 4;
			function autoscroll() {	
				let auto = setInterval(function() {
					let l = parseInt($(".mscroll ul").css("margin-left"));
					$(".mscroll ul").animate({
						marginLeft: l - v + "px"
					}, 10, function() {
						$(this).stop();
						if (l < -900) {
							$(".mscroll ul").css({
								marginLeft: "0px"
							})
						}
					});
				}, 100)
			}
			$(".mscroll ul li").mouseover(function(){
				v = 0;
				$(this).css("transform","scale(1.08)");
				$(this).css("box-shadow","0px 0px 13px 0px rgba(0, 0, 0, 0.2)")
			})
			$(".mscroll ul li").mouseout(function(){
				v = 4;
				$(this).css("transform","scale(1)");
			})
		},
		methods: {
			NavScrollFunction() {
				this.scrollnumber($('.home-site-title1'), 'font_active_up', 50, 1);
				this.scrollnumber($('.home-site-title2'), 'font_active_up', 50, 2);
				this.scrollnumber($('.home-site-title3'), 'font_active_up', 50, 3);
