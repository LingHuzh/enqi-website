.button_hover {
	position: relative;
	overflow: hidden;
}

.button_hover:after {
	content: "";
	background: rgba(255, 255, 255, 0.3);
	display: block;
	position: absolute;
	padding-top: 0;
	padding-left: 0;
	margin-top: 0;
	margin-left: 0;
	left: 50%;
	top: 50%;
	opacity: 1;
	transition: 0s;
	border-radius: 500px;
}

.button_hover:hover:after {
	border-radius: 500px;
	padding-top: 320%;
	padding-left: 320%;
	margin-top: -120%;
	margin-left: -160%;
	opacity: 0;
	transition: all 1.5s;
}

.ripple-container .ripple {
	background-color: rgba(255, 255, 255, 0.4);
	animation: ripple 2s forwards cubic-bezier(0, 0, 0.2, 1);
}

@keyframes ripple {
	0% {
		transform: scale(0);
		opacity: 1;
	}

	80% {
		transform: scale(1);
	}

	100% {
		opacity: 0;
	}
}

@-webkit-keyframes ripple {
	0% {
		transform: scale(0);
		opacity: 1;
	}

	80% {
		transform: scale(1);
	}

	100% {
		opacity: 0;
	}
}

@-moz-keyframes ripple {
	0% {
		transform: scale(0);
		opacity: 1;
	}

	80% {
		transform: scale(1);
	}

	100% {
		opacity: 0;
	}
}

@-o-keyframes ripple {
	0% {
		transform: scale(0);
		opacity: 1;
	}

	80% {
		transform: scale(1);
	}

	100% {
		opacity: 0;
	}
}

