.show_phone:hover {
	border-bottom: none !important;
	transform: translateY(-6px);
	transition-duration: .5s;
	-moz-transition-duration: .5s;
	-webkit-transition-duration: .5s;
	-o-transition-duration: .5s;
}

.show_phone:hover>ul {
	display: block;
}

.show_phone {
	position: relative;
}

.show_phone ul {
	position: absolute;
	width: 100px;
	display: inline-block;
	height: 46px;
	line-height: 70px;
	border-radius: 3px;
	background: #fff;
	top: 80px;
	left: 15px;
	display: none;
}

.show_phone ul li {
	height: 40px;
	line-height: 40px;
	text-align: center;
}

.show_phone ul li a {
	color: #333;
	font-size: 14px;
}

